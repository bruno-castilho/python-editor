{
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "editor.formatOnSave": true,
      "extensions": [
        "ms-azuretools.vscode-docker",
        "prisma.prisma"
      ]
    }
  },
  "forwardPorts": [
    3000
  ],
  "postCreateCommand": "npm install"
}